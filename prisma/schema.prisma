
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TradeOrder {
  id        String   @id @default(uuid())
  type      String
  quantity  Float
  price     Float
  timestamp DateTime @default(now())
  status    String
}

model Contact {
  id          String   @id @default(uuid())
  contactType String
  name        String
  email       String
  phone       String?
  createdAt   DateTime @default(now())
}